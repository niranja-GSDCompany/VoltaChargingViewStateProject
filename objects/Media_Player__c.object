    <validationRules>
        <fullName>MAC_Address_test</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(
OR(
ISBLANK (MAC_Address__c),
REGEX( MAC_Address__c ,&quot;^([0-9A-Fa-f]{2}[:-]){5}([0-9A-Fa-f]{2})$&quot;)
)
)</errorConditionFormula>
        <errorDisplayField>MAC_Address__c</errorDisplayField>
        <errorMessage>Must be a valid MAC address, eg. 3D:F2:C9:A6:B3:4F</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MAC_FORMAT</fullName>
        <active>true</active>
        <errorConditionFormula>NOT( 
OR( 
ISBLANK ( LAN_MAC_alt__c ), 
REGEX( LAN_MAC_alt__c  ,&quot;^([0-9A-Fa-f]{2}[:-]){5}([0-9A-Fa-f]{2})$&quot;) 
) 
)</errorConditionFormula>
        <errorDisplayField>LAN_MAC_alt__c</errorDisplayField>
        <errorMessage>Incorrect MAC Address Format</errorMessage>
    </validationRules>
